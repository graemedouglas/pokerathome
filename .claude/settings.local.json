{
  "env": {
    "DISABLE_NON_ESSENTIAL_MODEL_CALLS": "1"
  },
  "permissions": {
    "allow": [
      "Bash(pnpm --filter @pokerathome/server test)",
      "Bash(ls:*)",
      "Bash(pnpm --filter @pokerathome/server test:*)",
      "Bash(pnpm tsc:*)",
      "Bash(pnpm test:*)",
      "Bash(pnpm build:*)",
      "Bash(timeout 10 pnpm dev:*)",
      "Bash(timeout 8 pnpm dev:*)",
      "Bash(pnpm exec tsc:*)",
      "Bash(npx tsc:*)",
      "Bash(node -e:*)"
    ]
  },
  "autoUpdatesChannel": "latest"
}
