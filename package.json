{
  "name": "@pokerathome/root",
  "private": true,
  "description": "Self-hostable online poker for humans and robots",
  "scripts": {
    "dev": "pnpm --filter @pokerathome/server dev",
    "dev:ui": "pnpm --filter @pokerathome/ui dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "test": "pnpm --filter @pokerathome/server test",
    "e2e": "pnpm --filter @pokerathome/e2e test",
    "reset": "bash scripts/reset.sh",
    "reset:start": "bash scripts/reset.sh --start",
    "kill": "bash scripts/kill-dev.sh"
  },
  "devDependencies": {
    "prettier": "^3.4.0",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.24.0"
}
